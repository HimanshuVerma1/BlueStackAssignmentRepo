pipeline {

	agent any
	
	properties([
		parameters([
			string(
				defaultValue:'',
				name: 'Branch',
				trim: true,
				description: 'Enter branch name to build the branch'
			)
		])
	])
	
	stages {
		stage ('Check out the specific branch') {
			steps{
				git branch: "%params.Branch%", url: 'https://github.com/HimanshuVerma1/BlueStackAssignmentRepo.git'
			}
		}
	}
}